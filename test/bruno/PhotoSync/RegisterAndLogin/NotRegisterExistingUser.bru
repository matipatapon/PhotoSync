meta {
  name: NotRegisterExistingUser
  type: http
  seq: 4
}

post {
  url: {{HOST}}/register
  body: json
  auth: inherit
}

assert {
  res.status: eq 401
  res.body: eq ""
}

script:pre-request {
  var username = bru.getCollectionVar("USERNAME")
  var password = bru.getCollectionVar("PASSWORD")
  req.setBody(`{"username": "${username}", "password": "${password}"}`)
}
