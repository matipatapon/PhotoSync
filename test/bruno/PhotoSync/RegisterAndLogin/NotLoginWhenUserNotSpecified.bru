post {
  url: {{host}}/login
  body: json
  auth: inherit
}

assert {
  res.status: eq 401
  res.body: eq ""
}

script:pre-request {
  var password = bru.getCollectionVar("password")
  req.setBody(`{"password": "${password}"}`)
}
