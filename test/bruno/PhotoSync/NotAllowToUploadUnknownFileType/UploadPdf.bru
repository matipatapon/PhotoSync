meta {
  name: UploadPdf
  type: http
  seq: 4
}

post {
  url: {{HOST}}/upload
  body: multipartForm
  auth: inherit
}

headers {
  Authorization: {{TOKEN}}
}

body:multipart-form {
  filename: img.pdf
  modification_date: 2025.07.05 12:32:59
  file: @file(../../images/img.pdf)
}

assert {
  res.status: eq 401
  res.body: eq ""
}

settings {
  encodeUrl: true
}
