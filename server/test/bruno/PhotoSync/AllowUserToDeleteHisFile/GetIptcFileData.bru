meta {
  name: GetIptcFileData
  type: http
  seq: 10
}

get {
  url: {{HOST}}/file_data?offset=0&count=10
  body: none
  auth: inherit
}

params:query {
  offset: 0
  count: 10
}

headers {
  Authorization: {{TOKEN}}
}

assert {
  res.status: eq 200
}

tests {
  test("should return proper file data", function(){
    expect(res.getBody()).to.eql([
      {
        creation_date : "2022.03.01 21:37:00",
        filename : "iptc.jpg",
        id: "3",
        mime_type: "image/jpeg",
        size: "3240",
      },
    ])
  })
}

settings {
  encodeUrl: true
}
