meta {
  name: GetFileDataWithThumbnail
  type: http
  seq: 8
}

get {
  url: {{HOST}}/file_data?offset=0&count=10
  body: none
  auth: inherit
}

params:query {
  offset: 0
  count: 10
}

headers {
  Authorization: {{TOKEN}}
}

assert {
  res.status: eq 200
}

tests {
  test("should return proper file data", function(){
    expect(res.getBody()).to.eql([
      {
        creation_date : "2025.07.05 12:32:59",
        filename : "600_x_601.jpg",
        id: "1",
        mime_type: "image/jpeg",
        size: "13119",
        thumbnail: "",
      },
  
    ])
  })
}

settings {
  encodeUrl: true
}
