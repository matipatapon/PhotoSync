meta {
  name: UploadRawJpg
  type: http
  seq: 4
}

post {
  url: {{HOST}}/upload
  body: multipartForm
  auth: inherit
}

headers {
  Authorization: {{TOKEN}}
}

body:multipart-form {
  filename: raw.jpg
  modification_date: 2023.06.07 15:40:00
  file: @file(../../images/raw.jpg)
}

assert {
  res.status: eq 200
  res.body.creation_date: eq 2023.06.07 15:40:00
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

settings {
  encodeUrl: true
  timeout: 0
}
