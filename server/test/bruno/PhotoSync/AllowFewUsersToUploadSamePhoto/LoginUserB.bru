meta {
  name: LoginUserB
  type: http
  seq: 5
}

post {
  url: {{HOST}}/login
  body: none
  auth: inherit
}

assert {
  res.status: eq 200
}

script:pre-request {
  req.setBody(`{"username": "${bru.getCollectionVar("USERNAME2")}", "password": "${bru.getCollectionVar("PASSWORD2")}"}`)
}

script:post-response {
  bru.setVar("TOKENB", res.body)
}
