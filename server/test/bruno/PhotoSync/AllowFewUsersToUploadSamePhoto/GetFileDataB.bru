meta {
  name: GetFileDataB
  type: http
  seq: 9
}

get {
  url: {{HOST}}/file_data?date=2025.08.11
  body: none
  auth: inherit
}

params:query {
  date: 2025.08.11
}

headers {
  Authorization: {{TOKENB}}
}

tests {
  const rawJpgBase64 = bru.getCollectionVar("RAW_JPG_BASE64")
  test("should return proper file data", function(){
    expect(res.getBody()).to.eql(
    [
      {
        creation_date : "2025.08.11 15:40:00",
        filename : "raw.jpg",
        id: "2",
        mime_type: "image/jpeg",
        size: "2439",
        thumbnail: rawJpgBase64,
      },
    ]
    )
  })
  
}

settings {
  encodeUrl: true
}
