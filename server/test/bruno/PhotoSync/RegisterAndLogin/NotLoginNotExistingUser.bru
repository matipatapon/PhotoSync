meta {
  name: NotLoginNotExistingUser
  type: http
  seq: 11
}

post {
  url: {{HOST}}/login
  body: json
  auth: inherit
}

assert {
  res.status: eq 401
  res.body: eq ""
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

script:pre-request {
  var password = bru.getCollectionVar("PASSWORD")
  req.setBody(`{"username": "notexistinguser", "password": "${password}"}`)
}
