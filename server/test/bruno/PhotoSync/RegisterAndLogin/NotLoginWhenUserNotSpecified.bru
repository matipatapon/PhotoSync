meta {
  name: NotLoginWhenUserNotSpecified
  type: http
  seq: 12
}

post {
  url: {{HOST}}/login
  body: json
  auth: inherit
}

assert {
  res.status: eq 401
  res.body: eq ""
}

script:pre-request {
  var password = bru.getCollectionVar("PASSWORD")
  req.setBody(`{"password": "${password}"}`)
}
