meta {
  name: NotLoginUserWithoutPassword
  type: http
  seq: 10
}

post {
  url: {{HOST}}/login
  body: json
  auth: inherit
}

assert {
  res.status: eq 401
  res.body: eq ""
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

script:pre-request {
  var username = bru.getCollectionVar("USERNAME")
  req.setBody(`{"username": "${username}"}`)
}
