meta {
  name: UploadExifJpg
  type: http
  seq: 5
}

post {
  url: {{HOST}}/upload
  body: multipartForm
  auth: inherit
}

headers {
  Authorization: {{TOKEN}}
}

body:multipart-form {
  filename: exif.jpg
  modification_date: 2025.07.05 12:32:59
  file: @file(../../images/exif.jpg)
}

assert {
  res.status: eq 200
  res.body.creation_date: eq 2023.06.07 12:30:45
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

settings {
  encodeUrl: true
  timeout: 0
}
