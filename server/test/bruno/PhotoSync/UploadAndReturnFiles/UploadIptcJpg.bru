meta {
  name: UploadIptcJpg
  type: http
  seq: 4
}

post {
  url: {{HOST}}/upload
  body: multipartForm
  auth: inherit
}

headers {
  Authorization: {{TOKEN}}
}

body:multipart-form {
  filename: iptc.jpg
  modification_date: 2025.07.05 12:32:59
  file: @file(../../images/iptc.jpg)
}

assert {
  res.status: eq 200
  res.body.creation_date: eq 2022.03.01 21:37:00
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

settings {
  encodeUrl: true
  timeout: 0
}
