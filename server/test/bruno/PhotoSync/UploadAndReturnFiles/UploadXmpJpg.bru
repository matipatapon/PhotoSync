meta {
  name: UploadXmpJpg
  type: http
  seq: 6
}

post {
  url: {{HOST}}/upload
  body: multipartForm
  auth: inherit
}

headers {
  Authorization: {{TOKEN}}
}

body:multipart-form {
  filename: xmp.jpg
  modification_date: 2025.07.05 12:32:59
  file: @file(../../images/xmp.jpg)
}

assert {
  res.status: eq 200
  res.body.creation_date: eq 2007.12.20 00:24:13
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

settings {
  encodeUrl: true
  timeout: 0
}
