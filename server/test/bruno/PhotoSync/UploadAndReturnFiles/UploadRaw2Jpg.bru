meta {
  name: UploadRaw2Jpg
  type: http
  seq: 8
}

post {
  url: {{HOST}}/upload
  body: multipartForm
  auth: inherit
}

headers {
  Authorization: {{TOKEN}}
}

body:multipart-form {
  filename: raw2.jpg
  modification_date: 2023.08.03 23:30:00
  file: @file(../../images/raw2.jpg)
}

assert {
  res.status: eq 200
  res.body.creation_date: eq 2023.08.03 23:30:00
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

settings {
  encodeUrl: true
  timeout: 0
}
