meta {
  name: RegisterUser
  type: http
  seq: 2
}

post {
  url: {{HOST}}/register
  body: json
  auth: inherit
}

assert {
  res.status: eq 200
  res.body: eq ""
  res.headers["access-control-allow-origin"]: eq {{ALLOWED_ORIGIN}}
}

script:pre-request {
  req.setBody(`{"username": "${bru.getCollectionVar("USERNAME")}", "password": "${bru.getCollectionVar("PASSWORD")}"}`)
}
