meta {
  name: UploadUserAFile
  type: http
  seq: 6
}

post {
  url: {{HOST}}/upload
  body: multipartForm
  auth: inherit
}

headers {
  Authorization: {{TOKENA}}
}

body:multipart-form {
  filename: exif.jpg
  modification_date: 2025.08.11 15:40:00
  file: @file(../../images/exif.jpg)
}

assert {
  res.status: eq 200
  res.body.creation_date: eq 2023.06.07 12:30:45
}

settings {
  encodeUrl: true
  timeout: 0
}
